{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "mia-ui/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/chat-api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "mia-ui/$1"
    }
  ]
}
